\section{Features}
\begin{description}
\item[auto-spaced units]
	\[20\unit{u="\frac{V}{m^2}"}\]
\item[Integral macro]
	\[\integral{expr="f(x)", wrt=x, from=l, inf=1}\]
\item[Integral macro]
	\[\integral{expr="f(x)", wrt=x, from=l, inf=1}\]
\item[Summation macro]
	\[\summ{expr="x[k]", wrt=k, to=n, inf=1}\]
\item[Derivative macro]
	\[\deriv{wrt=x, of=y, n=2}\]
